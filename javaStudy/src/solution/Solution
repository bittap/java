package solution;

public class Solution {

	public static void main(String[] args) {

	}
	
    public int[] twoSum(int[] nums, int target) {
        for(int i = 0; i<nums.length; i++){
            for(int j = 1; j<nums.length; j++){
                int sum = nums[i]+nums[j];
                if(target == sum){
                    int[] targetIndex = new int[]{i,j};
                    return targetIndex;
                }
            }
        }
    }
}

}
